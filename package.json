{
  "name": "todo",
  "main": "./lib/main",
  "version": "0.1.0",
  "description": "Todo extension",
  "keywords": [],
  "repository": "https://github.com/cc/string-utils",
  "license": "MIT",
  "engines": {
    "cc": ">=1.0.0 <2.0.0"
  },
  "dependencies": {},
  "itemTypes": {
    "neux": {
      "todo": {
        "done": "boolean",
        "priority": "integer",
        "_meta": {
          "base": "core.text"
        }
      }
    }
  },
  "actionProviders": [
    {
      "name": "todo",
      "description": "Convert the string to a todo item.",
      "input": "text",
      "output": "neux.todo",
      "methodName": "todo",
      "onOutput": "replace"
    }
  ],
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "vueify": "^9.4.1"
  }
}
